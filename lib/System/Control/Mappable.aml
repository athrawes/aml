module Mappable =
  /**
   * @docs-from ./Mappable.docs.md#`map`
   */
  map: M<A> -> (A -> B) -> M<B>

  /**
   * @docs-from ./Mappable.docs.md#`<$>`
   * @include map
   */
  (<$>) = map

  /**
   * @docs-from ./Mappable.docs.md#`<&>`
   * @include map
   */
  (<&>) = swap-args map
