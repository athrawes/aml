module Applyable =
  /**
   * @docs-from ./Applyable.docs.md#`apply`
   */
  apply: M<A -> B> -> M<A> -> M<B>

  /**
   * @docs-from ./Applyable.docs.md#`<*>`
   * @include apply
   */
  <*> : infix = apply

  /**
   * @docs-from ./Applyable.docs.md#`<**>`
   * @include apply
   */
  <**> : infix = swap-args apply
