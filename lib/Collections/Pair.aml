{True, False, true, false} = use "Primitives/Boolean"

type Pair<'a, 'b> =
  | First of True -> 'a
  | Second of False -> 'b

# Constructs a new pair from two arguments
from : 'a -> 'b -> Pair<'a, 'b>
from = a b -> which -> which a b

# Grabs the first item from a Pair
fst : Pair<'a, 'b> -> 'a
fst = self -> self true

# Grabs the second item from a Pair
snd : Pair<'a, 'b> -> 'b
snd = self -> self false
