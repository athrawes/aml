pipe = (argument: 'a) (callback: ('a -> 'b)): 'b -> callback argument

(|>) = pipe
(<|) = flip (|>)

compose = (a_b: 'a -> 'b) (b_c: 'b -> 'c): ('a -> 'c) ->
  a -> a |> a_b |> b_c

(>>) = compose
(<<) = flip (>>)

flip = (callback: ('a -> 'b -> 'c)): ('b -> 'a -> 'c) ->
  b a -> callback a b

id = (a: 'a): 'a -> a
