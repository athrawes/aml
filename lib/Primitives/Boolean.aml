true =  a _ -> a
false = _ b -> b

type Boolean =
  | True of true
  | False of false

and : Boolean -> Boolean -> Boolean
(and) = a b -> a b a

or : Boolean -> Boolean -> Boolean
(or) = a b -> a a b

(xor) = is >> not

not : Boolean -> Boolean
not = condition -> flip condition

(is) : 'a -> 'a -> Boolean
