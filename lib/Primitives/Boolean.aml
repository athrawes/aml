true =  a _ -> a
false = _ b -> b

type :boolean =
  | :true of true
  | :false of false

and : :boolean -> :boolean -> :boolean
(and) = a b -> a b a

or : :boolean -> :boolean -> :boolean
(or) = a b -> a a b

(xor) = is >> not

not : :boolean -> :boolean
not = condition -> flip condition

(is) : 'a -> 'a -> :boolean
