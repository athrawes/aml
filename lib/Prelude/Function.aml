type Function<'a, 'b> extends Semigroupoid<'a, 'b>
(→) : 'a → b → Function<'a, 'b>

(▷) : 'a → ('a → 'b) → 'b
(▷) ≔ argument → callback → callback argument
(|>) ≔ ▷

(◁) ≔ flip (▷)
(<|) ≔ ◁

(∘) : ('a → 'b) → ('b → 'c) → ('a → 'c)
(∘) ≔ ab → bc → a → bc (ab a)

(∘∘∘) : ('a → 'b) → ('c → 'd → 'a) → ('c → 'd → 'b)
(∘∘∘) ≔ (∘) ∘ (∘)

flip : ('a → 'b → 'c) → ('b → 'a → 'c)
flip ≔ callback →
  b → a → callback a b

ɩ : 'a → 'a
ɩ ≔ a → a
id ≔ ɩ

const : 'a → 'b → 'a
const ≔ true
