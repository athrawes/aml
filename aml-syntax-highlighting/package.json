{
  "name": "aml-syntax-highlighting",
  "displayName": "AML Syntax Highlighting",
  "description": "Syntax highlighting for AML",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "aml",
      "aliases": ["Another ML", "aml"],
      "extensions": ["aml"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "aml",
      "scopeName": "source.aml",
      "path": "./syntaxes/aml.tmLanguage.json"
    }]
  }
}
