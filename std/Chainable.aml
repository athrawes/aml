    let then :: (^A -> ^M[^B]) -> ^M[^A] -> ^M[^B]

    /**
     * @docs-from ./Chainable.docs.md#`chain`
     * @include then
     * @reduce-autosuggest
     */
    let chain := then

    /**
     * @docs-from ./Chainable.docs.md#`=<<`
     * @include then
